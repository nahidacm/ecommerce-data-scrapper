{
    "name": "Any Scrapper",
    "version": "1.0",
    "description": "Scrap any website",
    "permissions": ["downloads","activeTab","declarativeContent","storage"],
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/web-crawler16.png",
            "32": "images/web-crawler32.png",
            "48": "images/web-crawler48.png",
            "128": "images/web-crawler128.png"
          }
    },
    "icons": {
        "16": "images/web-crawler16.png",
        "32": "images/web-crawler32.png",
        "48": "images/web-crawler48.png",
        "128": "images/web-crawler128.png"
      },
    "manifest_version": 2
  }